// Write a Java program to show parameterized constructor
//class Student {
    String name;
    int age;

    public Student(String name, int age) {
        this.name = name;
        this.age = age;
    }

    
    public void display() {
        System.out.println("Name: " + name);
        System.out.println("Age: " + age);
    }
}

public class Q32 {
    public static void main(String[] args) {
       
        Student student1 = new Student("karan", 20);
        Student student2 = new Student("sujal", 22);

       
        student1.display();
        student2.display();
    }
}

